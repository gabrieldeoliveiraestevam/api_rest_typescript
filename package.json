{
  "name": "api-rest-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "start": "node dist/index.js ",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify ./src/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:revert",
    "test": "jest --coverage --passWithNoTests --detectOpenHandles --runInBand",
    "test:staged": "jest --findRelatedTests",
    "test:push": "jest --coverage",
    "husky:prepare": "husky install"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.11",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^15.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^3.1.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "*"
  },
  "dependencies": {
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.11"
  }
}
